package Robot;

public class MyMath {
final static double pi=22/7;
final static double e=2.718;


static double ploshchaKruha(int x){
	
	double s=pi*pow(x,2);
	return s;
	
	
}

static int pow(int x, int s){
	int r;
	for (int i = 2; i <= s; i++) {
	if	(s==0) return 1;
	else if (s==1) return x;
	else r=x*x;	
	}
	
	return 0;
	
}

}
