package Deposit;

import java.util.Scanner;

public class Dividends {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("—к≥льки ¬ам рок≥в?");
		int year = sc.nextInt();
		System.out.println("¬ ск≥льки рок≥в плануЇте вийти на пенс≥ю?");
		int end = sc.nextInt();
		System.out.println("яку суму ¬и можете в≥дкласти в р≥к?");
		double money = sc.nextInt();
		sc.close();
		
		System.out.println(arrayInitialize(year, end));
		//arrayDividends(arr, money);
	}

	static double [] arrayInitialize (int year, int end) {
		 double arr[]=new double[end-year];
		 return arr;
		}
		
static void arrayDividends(double arr[], double money){
		for (int i = 0; i < arr.length; i++) {
			arr[i]=money*1.19;
			money=arr[i];
			System.out.println(arr[i]);
		}
		
		
	}
}
