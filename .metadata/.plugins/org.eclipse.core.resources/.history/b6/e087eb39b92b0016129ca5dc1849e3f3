package Robot;

public class MyMath {
final static double pi=22/7;
final static double e=2.718;


static double ploshchaKruha(int x){
	
	double s=pi*myPow(x,2);
	return s;
	
	
}

static int myPow(int x, int s){
	int r=x;
	for (int i = 0; i <= s-2; i++) {
	if	(s==0) return 1;
	else if (s==1) return x;
	else r=r*x;	
	}
	
	return r;
	
}

}
