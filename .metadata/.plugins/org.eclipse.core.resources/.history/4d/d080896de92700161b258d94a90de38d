	static public int myCompareTo(String str1, String str2) {

		String str;
		char[] ch1 = str1.toCharArray();
		char[] ch2 = str2.toCharArray();
		
		if (str1.length() > str2.length())
			str = str2;
		else
			str = str1;

		for (int i = 0; i < str.length(); i++) {
			
			if(ch1[i]!=ch2[i])
			return ch1[i]-ch2[i];
			
		}

		return 0;
	}
}