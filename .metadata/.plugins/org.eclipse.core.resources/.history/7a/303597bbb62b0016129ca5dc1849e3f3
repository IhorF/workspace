package Robot;

public class MyMath {
final static double pi=22/7;
final static double e=2.718;


static double ploshchaKruha(int r){
	
	double s=pi*pow(r,2);
	return s;
	
	
}

static double pow(double r, int s){
	for (int i = 0; i <= s; i++) {
	if	(s==0) return 1;
	else if (s==1) return r;
	else r*=r;	
	}
	
	return r;
	
}

}
